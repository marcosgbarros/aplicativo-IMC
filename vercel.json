{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/scripts/(.*)",
        "dest": "/scripts/$1"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  